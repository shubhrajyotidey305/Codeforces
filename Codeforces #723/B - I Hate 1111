#include<bits/stdc++.h>
using namespace std;
#define ll long long

void solve() {
    int x;
    cin >> x;

    while(true){
        if(x%11==0){
            cout << "YES";
            return;
        }
        else x-=111;

        if(x<0) break;
    }
    cout << "NO";
    
}

int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(NULL);
    cout.tie(NULL);
    
    int t;
    cin >> t;

    while(t--){
        solve();
        cout << endl;
    }
    return 0;
}
